{
  "name": "react-vite-ts-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=24.11.0",
    "pnpm": ">=10.24.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "format": "prettier --write src/**/*.{css,ts,tsx}",
